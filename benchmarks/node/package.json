{
  "name": "sheetkit-benchmark",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "generate": "tsx generate-fixtures.ts",
    "bench": "node --expose-gc --import tsx benchmark.ts",
    "bench:all": "node --expose-gc --import tsx benchmark.ts && node --expose-gc --import tsx benchmark.ts --format=markdown"
  },
  "dependencies": {
    "@sheetkit/node": "workspace:*",
    "exceljs": "^4.4.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "tsx": "^4.19.0",
    "typescript": "^5.9.0"
  }
}
