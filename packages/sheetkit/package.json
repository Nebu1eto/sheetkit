{
  "name": "@sheetkit/node",
  "version": "0.2.0",
  "license": "MIT OR Apache-2.0",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "napi": {
    "binaryName": "sheetkit",
    "targets": [
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-pc-windows-msvc",
      "aarch64-pc-windows-msvc",
      "x86_64-unknown-linux-gnu",
      "aarch64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl",
      "aarch64-unknown-linux-musl"
    ]
  },
  "files": [
    "index.js",
    "index.d.ts",
    "binding.js",
    "binding.d.ts",
    "buffer-codec.js",
    "buffer-codec.d.ts",
    "sheet-data.js",
    "sheet-data.d.ts"
  ],
  "author": {
    "name": "Haze Lee",
    "url": "https://github.com/Nebu1eto"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nebu1eto/sheetkit"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build:napi": "napi build --platform --release --esm --js binding.js --dts binding.d.ts",
    "build:js": "swc index.ts buffer-codec.ts sheet-data.ts -d .",
    "build:types": "tsc --project tsconfig.build.json",
    "build": "pnpm build:napi && pnpm build:js && pnpm build:types",
    "build:debug": "napi build --platform --js binding.js --dts binding.d.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "vitest run",
    "test:watch": "vitest",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.5.1",
    "@swc/cli": "^0.8.0",
    "@swc/core": "^1.15.11",
    "@types/node": "^25.2.2"
  },
  "optionalDependencies": {
    "@sheetkit/node-darwin-arm64": "0.2.0",
    "@sheetkit/node-darwin-x64": "0.2.0",
    "@sheetkit/node-linux-arm64-gnu": "0.2.0",
    "@sheetkit/node-linux-arm64-musl": "0.2.0",
    "@sheetkit/node-linux-x64-gnu": "0.2.0",
    "@sheetkit/node-linux-x64-musl": "0.2.0",
    "@sheetkit/node-win32-arm64-msvc": "0.2.0",
    "@sheetkit/node-win32-x64-msvc": "0.2.0"
  }
}
